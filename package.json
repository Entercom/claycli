  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint": "^4.17.0",
    "jest": "^22.4.1"
  },
  "dependencies": {
    "clayutils": "^2.0.3",
    "highland": "^2.10.5",
    "js-yaml": "^3.8.4",
    "lodash": "^4.17.4",
    "node-fetch": "^1.6.3",
